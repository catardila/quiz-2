Algoritmo NumMayo
	definir num1, num2 Como Entero
	Escribir "Ingrese el primer numero"
	leer num1
	Escribir "Ingrese el segundo numero"
	leer num2
	si num1 > num2 entonces 
		escribir num1 " es mayor que " num2
	SiNo
		si num1 < num2 Entonces
			Escribir num2 " es mayor que " num1
		SiNo
			si num1 = num2 Entonces
				Escribir " Son iguales"
			FinSi
		FinSi
	FinSi
	
FinAlgoritmo
